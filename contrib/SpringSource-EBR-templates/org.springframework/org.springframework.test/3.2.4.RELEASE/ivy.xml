<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.springframework" module="org.springframework.test"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="javax.activation" name="com.springsource.javax.activation" rev="1.1.0" conf="provided->compile"/>
		<dependency org="javax.portlet" name="com.springsource.javax.portlet" rev="2.0.0" conf="provided->compile"/>
		<dependency org="javax.servlet" name="com.springsource.javax.servlet" rev="2.5.0" conf="provided->compile"/>
		<dependency org="javax.servlet" name="com.springsource.javax.servlet.jsp" rev="2.1.0" conf="provided->compile"/>
		<dependency org="org.junit" name="com.springsource.org.junit" rev="4.10.0" conf="optional->compile"/>
		<dependency org="org.testng" name="com.springsource.org.testng" rev="6.5.2" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.beans" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.context" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.core" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.jdbc" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.orm" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.transaction" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.web" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.web.portlet" rev="3.2.4.RELEASE" conf="optional->compile"/>
		<dependency org="org.springframework" name="org.springframework.web.servlet" rev="3.2.4.RELEASE" conf="optional->compile"/>
	</dependencies>

</ivy-module>
