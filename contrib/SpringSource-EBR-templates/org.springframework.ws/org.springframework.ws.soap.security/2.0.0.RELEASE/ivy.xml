<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.springframework.ws" module="org.springframework.ws.soap.security"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
		<artifact name="notice" type="notice" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="javax.xml.soap" name="com.springsource.javax.xml.soap" rev="1.3.0" conf="provided->runtime"/>
		<dependency org="net.sourceforge.ehcache" name="com.springsource.net.sf.ehcache" rev="2.0.0" conf="optional->runtime"/>
		<dependency org="org.apache.axiom" name="com.springsource.org.apache.axiom" rev="1.2.5" conf="optional->runtime"/>
		<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.logging" rev="1.1.1" conf="compile->runtime"/>
		<dependency org="org.apache.ws" name="com.springsource.org.apache.ws.security" rev="1.5.8" conf="optional->runtime"/>
		<dependency org="org.apache.xml" name="com.springsource.org.apache.xml.security" rev="1.0.5.D2" conf="optional->runtime"/>
		<dependency org="org.springframework" name="org.springframework.beans" rev="3.0.5.RELEASE" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.context" rev="3.0.5.RELEASE" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.core" rev="3.0.5.RELEASE" conf="compile->runtime"/>
		<dependency org="org.springframework.security" name="org.springframework.security.core" rev="2.0.5.RELEASE" conf="compile->runtime"/>
		<dependency org="org.springframework" name="org.springframework.transaction" rev="3.0.5.RELEASE" conf="compile->runtime"/>
		<dependency org="org.springframework.ws" name="org.springframework.ws" rev="2.0.0.RELEASE" conf="compile->runtime"/>
	</dependencies>

</ivy-module>
